{
  "name": "@davdevs/paper-react",
  "version": "1.0.0",
  "description": "React components and hooks for DavDevs Paper Design System",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "clean": "rimraf dist",
    "prepack": "npm run build"
  },
  "keywords": [
    "react",
    "hooks",
    "components",
    "ui",
    "design-system",
    "paper"
  ],
  "author": "Davina Leong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davinaleong/proj-davdevs-paper.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/davinaleong/proj-davdevs-paper/issues"
  },
  "homepage": "https://github.com/davinaleong/proj-davdevs-paper#readme",
  "dependencies": {
    "@davdevs/paper-foundations": "^1.0.0",
    "@davdevs/paper-core": "^1.0.0",
    "@davdevs/paper-basic": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "optionalDependencies": {
    "@davdevs/paper-premium": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
