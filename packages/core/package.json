{
  "name": "@davdevs/paper-core",
  "version": "1.0.0",
  "description": "Core CSS foundation for DavDevs Paper Design System",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./css": "./src/paper.css"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prepack": "npm run build"
  },
  "keywords": [
    "css",
    "core",
    "design-system",
    "paper",
    "ui"
  ],
  "author": "Davina Leong",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davinaleong/proj-davdevs-paper.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/davinaleong/proj-davdevs-paper/issues"
  },
  "homepage": "https://github.com/davinaleong/proj-davdevs-paper#readme",
  "dependencies": {
    "@davdevs/paper-foundations": "^1.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "vite": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
